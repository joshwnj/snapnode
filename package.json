{
  "name": "snapnode",
  "version": "0.6.0",
  "description": "run your node in snapshot mode",
  "main": "lib/index.js",
  "bin": {
    "snapnode": "bin/snapnode.js"
  },
  "scripts": {
    "babel": "npm run babel:prep && npm run babel:build",
    "babel:prep": "rm -rf lib && mkdir lib",
    "babel:build": "babel --copy-files -d lib src",
    "prepublishOnly": "npm test && npm run babel",
    "lint": "standard",
    "start": "electron .",
    "test": "npm run lint"
  },
  "keywords": [],
  "author": "joshwnj",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-cmz-names": "^1.3.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "chokidar": "^1.7.0",
    "cmz": "^3.6.1",
    "diff": "^3.3.1",
    "fresh-require": "^1.0.3",
    "mkdirp": "^0.5.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "relative-date": "^1.1.3"
  },
  "directories": {
    "lib": "lib",
    "src": "src"
  }
}
